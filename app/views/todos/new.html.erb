<head> <%= stylesheet_link_tag 'bootstrap.min' %> </head>
<h1>Create a New Todo</h1>
<%= form_for @todo do |f| %>

  <%= label_tag(:title, "Title:") %>
  <%= f.text_field :title %></br>

  <%= label_tag(:description, "Description") %> 
  <%= f.text_area :description %></br>
  <%= f.submit "Create" %>
<% end %>